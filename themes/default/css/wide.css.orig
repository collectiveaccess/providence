/* table in general */

table.listtable {
	background-color: #CDCDCD;
	margin:10px 0pt 15px;
	font-size: 8pt;
	width: 100%;
	text-align: left;
}

/* table headers */

table.listtable thead tr th {
	border: 1px solid #FFF;
	font-size: 8pt;
	padding: 4px 15px 4px 4px;
	background-repeat: no-repeat;
	background-position: center right;
	cursor: pointer;
}

th.list-header-unsorted {
