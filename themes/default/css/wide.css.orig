/*** Button styles ***/
div.button{
	padding-left:10px;
	font-size: 11px;
}
div.button a{
	color: #134959;
	text-decoration: none;
	font-size: 11px;
	font-weight:bold;
}

div.button a:hover{
	color: #1FAA99;
}
div.button img{
